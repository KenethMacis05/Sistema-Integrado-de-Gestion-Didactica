@{
    ViewBag.Title = "Dashboard";
}

<div class="alert text-center mt-4" style="background-color: #F7F7F7; border: 1px #D2D2D2 solid " role="alert"><h2 class="fs-3">Dashboard</h2></div>

<div class="jumbotron">
    <h1 class="display-4">Bienvenido</h1>
    <p class="lead">@ViewBag.NombreUsuario</p>
    @if (ViewBag.Mensaje != null)
    {
        <div class="alert alert-danger">
            @ViewBag.Mensaje
        </div>
    }    
</div>
